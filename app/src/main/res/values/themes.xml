<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AlarmProject" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/object_700</item>
        <item name="colorPrimaryVariant">@color/Primary_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/Primary_700</item>
        <item name="colorSecondaryVariant">@color/Primary_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/Primary_700</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="switchStyle">
        <item name="colorControlActivated">@color/object_700</item>
    </style>
</resources>